[ISMIP-HOM-A]
temperature = -30. 1.67e-5

[parameters]
geothermal = -42e-3
flow_factor = 1
ice_limit = 0
[CF output]
variables = thk usurf uvelhom vvelhom velnormhom
frequency = 1
name = output/hump.out.nc

[grid]
upn = 2
ewn = 160
nsn = 160
dew = 250
dns = 250
sigma_builtin = 1

[time]
niso = 1.
tend = 100.
ntem = 1.
tstart = 0.
dt = 1.
nvel = 1.

[CF default]
comment = forced upper kinematic BC
title = EISMINT-2 Exp B

[options]
flow_law = 2
evolution = 0
temperature = 1
vertical_integration = 1
sliding_law = 4
marine_margin = 2
stress_calc = 2
hotstart = 1
basal_water = 0
isostasy = 0
diagnostic_run = 1

[ho_options]
diagnostic_scheme = 3
prognostic_scheme = 0
which_ho_babc = 4
which_ho_efvs = 0
which_ho_resid = 0
which_ho_sparse = 1

[CF input]
name = hump.bisicles.nc
time = 1

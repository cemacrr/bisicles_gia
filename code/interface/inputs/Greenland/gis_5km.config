[ISMIP-HOM-A]
temperature = -30. 1.67e-5

[parameters]
geothermal = -42e-3
flow_factor = 1
default_flwa = 1.0e-16

[CF output]
variables = thk topg artm acab temp bheatflx bmlt bwat uvelhom vvelhom velnormhom kinbcmask beta
frequency = 5
name = gis_5km.out.nc

[grid]
upn = 11
ewn = 301
nsn = 561
dew = 5000
dns = 5000
sigma_builtin = 1

[time]
tstart = 0.0
tend = 5000.0
dt = 10.

[CF default]
comment = Greenland high resolution run
title = Greenland, 5km res

[options]
flow_law = 0        #Isothermal = 2
evolution = 0
temperature = 1
vertical_integration = 0
sliding_law = 4
marine_margin = 2
stress_calc = 2
hotstart = 0
basal_water = 0
isostasy = 0
periodic_ew = 0
periodic_ns = 0
diagnostic_run = 1

[ho_options]
diagnostic_scheme = 3
prognostic_scheme = 0
basal_stress_input = 3
which_ho_nonlinear = 0
which_ho_efvs = 0
which_ho_source = 0
which_ho_sparse = 1
which_ho_babc = 4
which_ho_resid = 1
which_disp = 0
which_bmelt = 0

[CF input]
name = gis_5km.nc
time = 1

[external_dycore_options]
external_dycore_type = 0
dycore_input_file = ../../../BISICLES/code/interface/inputs.glimmer.gis

